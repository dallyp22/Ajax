# Google Cloud Configuration
GOOGLE_APPLICATION_CREDENTIALS_BASE64=your-base64-encoded-service-account-json-here
# OR use file path (for local development)
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account.json

# BigQuery Configuration
BIGQUERY_PROJECT_ID=your-bigquery-project-id
RENTROLL_TABLE_ID=your-project.dataset.rentroll_table
COMPETITION_TABLE_ID=your-project.dataset.competition_table

# Application Settings
DEBUG=false
LOG_LEVEL=INFO
CORS_ORIGINS=https://your-frontend-domain.vercel.app,http://localhost:3000

# Port (Railway will set this automatically)
PORT=8000

# API Configuration
HOST=0.0.0.0
API_PREFIX=/api/v1

# BigQuery Datasets
BIGQUERY_DATASET_STAGING=staging
BIGQUERY_DATASET_MART=mart

# Optimization Engine Settings
DEFAULT_ELASTICITY=-0.003
MAX_PRICE_ADJUSTMENT=0.25
SIMILARITY_THRESHOLD=50.0
MAX_COMPS_PER_UNIT=10

# Performance Settings
MAX_CONCURRENT_OPTIMIZATIONS=100
CACHE_TTL_SECONDS=3600

# Security
ACCESS_TOKEN_EXPIRE_MINUTES=30 